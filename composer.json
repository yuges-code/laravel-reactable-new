{
    "name": "yuges-code/laravel-reactable",
    "description": "üëç Package for easily attaching reactions to Laravel eloquent models",
    "keywords": [
        "php",
        "yuges",
        "laravel",
        "package",
        "template",
        "reaction",
        "reactable"
    ],
    "homepage": "https://yuges-code.github.io/laravel-reactable",
    "support": {
        "source": "https://github.com/yuges-code/laravel-reactable",
        "issues": "https://github.com/yuges-code/laravel-reactable/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Georgy",
            "email": "goshasafonov@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "yuges-code/laravel-package": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Yuges\\Reactable\\": "src",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuges\\Reactable\\Tests\\": "tests/src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yuges\\Reactable\\Providers\\ReactableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
